<%- include('header') %>
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">

                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <strong>Basic Profile</strong>
                            </div>
                            <div class="card-body card-block">
                                <form action="/insertrecord" method="post" enctype="multipart/form-data" class="form-horizontal">

                                    <div class="col col-md-4">
                                        <img src="/images/ch.jpg" alt="" class="rounded-circle rounded-5">
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label for="text-input" class=" form-control-label">Name</label>
                                        </div>
                                        <div class="col-12 col-md-5">
                                            <h5 name="first_name" class="form-text text-muted">
                                                <%= userData.name%>
                                            </h5>
                                            <small class="form-text text-muted"></small>
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label for="email-input" class=" form-control-label"> Email</label>
                                        </div>
                                        <div class="col-12 col-md-5">
                                            <h5 class="form-text text-muted">
                                                <%= userData.email %>
                                            </h5>
                                            <small class="help-block form-text"></small>
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label class=" form-control-label">Date of Birth</label>
                                        </div>
                                        <div class="col col-md-5">
                                            <div class="radio">
                                                <label for="radio1">20 Sep 1999</label></div>
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label class=" form-control-label">Gender</label>
                                        </div>
                                        <div class="col col-md-5">
                                            <div class="radio">
                                                <label for="radio1">Male</label></div>
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label class=" form-control-label">Hobby</label>
                                        </div>
                                        <div class="col col-md-5">

                                            <label for="inline-radio2" class="form-check-label ">Music</label>
                                            <label for="inline-radio3" class="form-check-label ">Traveling</label>
                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label class=" form-control-label">City</label>
                                        </div>
                                        <div class="col col-md-5">

                                            <label for="inline-radio2" class="form-check-label ">Surat</label>

                                        </div>
                                    </div>

                                    <div class="row form-group">
                                        <div class="col col-md-3">
                                            <label for="textarea-input" class=" form-control-label">Description</label>
                                        </div>
                                        <div class="col-12 col-md-5">
                                            <label for="inline-radio3" class="form-check-label" style="text-align: justify"> An admin panel enables administrators of an application, website, or IT system to manage its configurations, settings, content, and features and carry out oversight functions critical to the business. It allows them to view the state of the platform and take any action in the performance of their duties</label>
                                        </div>
                                    </div>

                                    <div class="card-footer">
                                        <button type="submit" name="submit" class="btn btn-primary btn-sm">
                                                        <i class="fa fa-dot-circle-o"></i> Submit
                                                    </button>
                                        <button type="reset" class="btn btn-danger btn-sm">
                                                        <i class="fa fa-ban"></i> Reset
                                                    </button>
                                    </div>
                                </form>
                            </div>

                        </div>

                    </div>


                </div>
                <%- include('footer') %>